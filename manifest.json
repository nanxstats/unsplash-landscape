{
  "manifest_version": 3,
  "name": "Unsplash Landscape",
  "version": "1.0",
  "description": "Show only landscape images and exclude Unsplash+ on Unsplash.com",
  "permissions": [
    "storage"
  ],
  "action": {
    "title": "Unsplash Landscape",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "512": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://unsplash.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://unsplash.com/*"
  ]
}